            "content": "{%- if product.metafields.my_fields.msds_en %}<a href=\"{{ product.metafields.my_fields.msds_en.value.url }}\" target=\"_blank\">ðŸ‡¬ðŸ‡§ English<\/a><br>{%- endif -%}\n{%- if product.metafields.my_fields.msds_no %}<a href=\"{{ product.metafields.my_fields.msds_no.value.url }}\" target=\"_blank\">ðŸ‡³ðŸ‡´ Norwegian<\/a><br>{%- endif -%}\n{%- if product.metafields.my_fields.msds_se %}<a href=\"{{ product.metafields.my_fields.msds_se.value.url }}\" target=\"_blank\">ðŸ‡¸ðŸ‡ª Swedish<\/a>{%- endif -%}",
